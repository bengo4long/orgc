<?xml version="1.0" encoding="UTF-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Quantity_Except_IntegralValue</fullName>
    <active>true</active>
    <errorConditionFormula>mod(Quantity, 1) &lt;&gt; 0</errorConditionFormula>
    <errorDisplayField>Quantity</errorDisplayField>
    <errorMessage>整数をいれてください</errorMessage>
</ValidationRule>
