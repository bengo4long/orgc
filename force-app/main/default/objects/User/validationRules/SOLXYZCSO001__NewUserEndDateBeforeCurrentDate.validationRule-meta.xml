<?xml version="1.0" encoding="UTF-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>SOLXYZCSO001__NewUserEndDateBeforeCurrentDate</fullName>
    <active>true</active>
    <errorConditionFormula>AND( ISNEW(), SOLXYZCSO001__ActiveEndDate__c &lt; DATEVALUE(NOW()))</errorConditionFormula>
    <errorDisplayField>SOLXYZCSO001__ActiveEndDate__c</errorDisplayField>
    <errorMessage>利用終了日時は、現在よりも後の日時を設定してください。</errorMessage>
</ValidationRule>
