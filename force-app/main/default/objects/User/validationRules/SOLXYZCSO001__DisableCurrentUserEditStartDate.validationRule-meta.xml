<?xml version="1.0" encoding="UTF-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>SOLXYZCSO001__DisableCurrentUserEditStartDate</fullName>
    <active>true</active>
    <errorConditionFormula>AND( ISCHANGED( SOLXYZCSO001__ActiveStartDate__c ) , Id  =  $User.Id)</errorConditionFormula>
    <errorDisplayField>SOLXYZCSO001__ActiveStartDate__c</errorDisplayField>
    <errorMessage>自分自身の開始日を編集することはできません。</errorMessage>
</ValidationRule>
