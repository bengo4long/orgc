<?xml version="1.0" encoding="UTF-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>SOLXYZCSO001__DisableCurrentUserEditLock</fullName>
    <active>true</active>
    <errorConditionFormula>AND( ISCHANGED( SOLXYZCSO001__Locked__c  ) , Id  =  $User.Id)</errorConditionFormula>
    <errorDisplayField>SOLXYZCSO001__Locked__c</errorDisplayField>
    <errorMessage>ログイン中のユーザは自分をロックまたはアンロックすることはできません。</errorMessage>
</ValidationRule>
