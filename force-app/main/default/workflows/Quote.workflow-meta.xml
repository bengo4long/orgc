<?xml version="1.0" encoding="UTF-8"?>
<Workflow xmlns="http://soap.sforce.com/2006/04/metadata">
    <alerts>
        <fullName>QuoteRejectEmail</fullName>
        <description>見積承認却下時の差し戻しメール</description>
        <protected>false</protected>
        <recipients>
            <type>creator</type>
        </recipients>
        <senderType>CurrentUser</senderType>
        <template>unfiled$public/QuoteApprovalRejectTemplate</template>
    </alerts>
    <fieldUpdates>
        <fullName>QuoteStatusApprovalDuringUpdate</fullName>
        <field>Status</field>
        <literalValue>承認待ち</literalValue>
        <name>見積の状況を承認中へ更新</name>
        <notifyAssignee>false</notifyAssignee>
        <operation>Literal</operation>
        <protected>false</protected>
        <reevaluateOnChange>false</reevaluateOnChange>
    </fieldUpdates>
    <fieldUpdates>
        <fullName>QuoteStatusApprovedUpdate</fullName>
        <field>Status</field>
        <literalValue>承認済</literalValue>
        <name>見積の状況を承認済へ更新</name>
        <notifyAssignee>false</notifyAssignee>
        <operation>Literal</operation>
        <protected>false</protected>
        <reevaluateOnChange>false</reevaluateOnChange>
    </fieldUpdates>
    <fieldUpdates>
        <fullName>QuoteStatusCreatingUpdate</fullName>
        <field>Status</field>
        <literalValue>作成中</literalValue>
        <name>見積の状況を作成中へ更新</name>
        <notifyAssignee>false</notifyAssignee>
        <operation>Literal</operation>
        <protected>false</protected>
        <reevaluateOnChange>false</reevaluateOnChange>
    </fieldUpdates>
    <fieldUpdates>
        <fullName>QuoteStatusRejectedUpdate</fullName>
        <field>Status</field>
        <literalValue>却下</literalValue>
        <name>見積の状況を却下へ更新</name>
        <notifyAssignee>false</notifyAssignee>
        <operation>Literal</operation>
        <protected>false</protected>
        <reevaluateOnChange>false</reevaluateOnChange>
    </fieldUpdates>
    <fieldUpdates>
        <fullName>UpdateEstimateApprovalIntoTrue</fullName>
        <field>QuoteApproval__c</field>
        <literalValue>1</literalValue>
        <name>見積承認をtrueへ更新</name>
        <notifyAssignee>false</notifyAssignee>
        <operation>Literal</operation>
        <protected>false</protected>
        <reevaluateOnChange>false</reevaluateOnChange>
    </fieldUpdates>
</Workflow>
